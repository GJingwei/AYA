%let path = D:\京都大学大学院様_最新202107分;
libname PC "D:\個人フォルダ\202203Gao";


data PC.demographics;
       %let _EFIERR_ = 0; /* エラー検出のマクロ変数を設定します */
       infile "&path.\母数.csv" delimiter = ',' MISSOVER DSD lrecl=32767 firstobs=2 ;
         informat VAR1 $20. ;
         informat VAR2 $8. ;
         informat VAR3 $16. ;
         informat VAR4 $20. ;
         informat VAR5 $16. ;
         informat VAR6 $64. ;
         informat VAR7 $8. ;
         informat VAR8 $8. ;
         informat VAR9 $3. ;
         format VAR1 $20. ;
         format VAR2 $8. ;
         format VAR3 $16. ;
         format VAR4 $20. ;
         format VAR5 $16. ;
         format VAR6 $64. ;
         format VAR7 $8. ;
         format VAR8 $8. ;
         format VAR9 $3. ;
      input
                  VAR1
                  VAR2
                  VAR3
                  VAR4
                  VAR5
                  VAR6
                  VAR7
                  VAR8
                  VAR9
      ;
      if _ERROR_ then call symputx('_EFIERR_',1);  /* set ERROR detection macro variable */
run;
